package com.nfeanalytics.core.helper;

import java.math.BigDecimal;

public class ImpostosHelper {

	private BigDecimal	vICMS;

	private BigDecimal	vST;

	private BigDecimal	vII;

	private BigDecimal	vIPI;

	private BigDecimal	vPIS;

	private BigDecimal	vCOFINS;

	public BigDecimal getvICMS() {
		return vICMS;
	}

	public void setvICMS(BigDecimal vICMS) {
		this.vICMS = vICMS;
	}

	public BigDecimal getvST() {
		return vST;
	}

	public void setvST(BigDecimal vST) {
		this.vST = vST;
	}

	public BigDecimal getvII() {
		return vII;
	}

	public void setvII(BigDecimal vII) {
		this.vII = vII;
	}

	public BigDecimal getvIPI() {
		return vIPI;
	}

	public void setvIPI(BigDecimal vIPI) {
		this.vIPI = vIPI;
	}

	public BigDecimal getvPIS() {
		return vPIS;
	}

	public void setvPIS(BigDecimal vPIS) {
		this.vPIS = vPIS;
	}

	public BigDecimal getvCOFINS() {
		return vCOFINS;
	}

	public void setvCOFINS(BigDecimal vCOFINS) {
		this.vCOFINS = vCOFINS;
	}

}
